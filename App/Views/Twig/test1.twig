<h1>Hello Twig example</h1>

{% if users|length > 0 %}
    <table border="1" cellpadding="5" cellspacing="0">
        {% for user in users %}
            <tr>
                <td>{{ user.id }}</td>
                <td>{{ user.fio }}</td>
                <td>{{ user.login }}</td>
                <td>{{ user.created_at }}</td>
                <td>{{ user.updated_at }}</td>
            </tr>
        {% endfor %}
    </table>
{% endif %}
